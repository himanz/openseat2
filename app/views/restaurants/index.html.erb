<p><%= link_to "New Restaurant", new_restaurant_path %> | <%= link_to "New User", new_user_path %></p>
<% if current_user %>
  <p><%= link_to "User Info", user_url(current_user) %></p>
<% end %>
<h1>Index of Restaurants</h1>
<% @restaurants.each do |restaurant| %>
	<p>Name: <%= link_to restaurant.name, restaurant_path(restaurant) %></p>
	<p>Capacity: <%= restaurant.capacity %></p>
<% end %>	